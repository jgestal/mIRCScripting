alias wput { echo -s $c10(Whois) $1- }
alias wputh { echo -s $c10(Whois) $1- | halt }
alias wcanales.comunes {
  unset %i %tmp.cchans.* | :ini | inc %i 
  if ($comchan($1,%i) != $null) {
    set %tmp.cchans.2 %tmp.cchans.1
    set %tmp.cchans.1 %tmp.cchans.2 $comchan($1,%i)
  goto ini }
return %tmp.cchans.1 }
alias pais { if ($right($1,3) == .es) return España.
  if ($right($1,3) == .fr) return Francia.
  if ($right($1,3) == .it) return Italia.
  if ($right($1,7) == .com.cl) return Chile.
  if ($right($1,3) == .pe) return Peru.
  if ($right($1,3) == .br) return Brasil.
  if ($right($1,7) == .com.ar) return Argentina.
  if ($right($1,7) == .net.mx) return Mexico.
  if ($right($1,3) == .uk) return Inglaterra.
  if ($right($1,7) == .com.co) return Colombia.
  if ($right($1,7) == .net.do) return República Dominicana.
  if ($right($1,3) == .au) return Australia.
  if ($right($1,3) == .jp) return Japón. 
}
