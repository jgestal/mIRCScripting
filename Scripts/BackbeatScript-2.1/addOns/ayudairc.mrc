on 1: connect: { .timerADDONNEW 1 10 cogever }
alias addon {
  echo -s 15,15·14,14·0,2 Ayuda Irc add on 14,14·15,15·
  echo -s 2 Versión:12 4.1       2Fecha:12 27-06-2000
  echo -s 2 Autor:12 Celsiuss
  echo -s 2 Email:12 Si tienes alguna sugerencia hazmela saber en addon@celsiuss.8m.com
  echo -s 2 Web:12 http://celsiuss.8m.com o www.arede.com/informatica/celsiuss
  echo -s 2 Distribuye libremente este ADDON pero por favor no cambies los datos
  echo -s 2 Puedes encontarme en 7#Ayuda_irc2 del hispano.org
}
alias cogever {
  if $group(#BOTS) == off { .enable #BOTS }
  sockclose addonnew
  sockopen addonnew www.arede.com 80
}
on 1:sockopen:addonnew: {
  if ( %numero.addon == $null ) {
    if $sockerr > 0 { echo -s Imposible ver actualizaciones | return }
    set -u40 %numero.addon ayuda
  sockwrite -tn addonnew GET /informatica/celsiuss/tayuda.txt }
  else { return }
}
on 1:sockread:addonnew: {
  if $sockerr > 0 { echo -s Imposible lectura | return }
  :lecturaddon
  sockread %testo.caddon
  if ($sockbr == 0) { goto finalectura }
  if ( %testo.caddon == $null ) { set %testo.caddon - }
  if (( --- isin %testo.caddon ) || ( HTTP/ isin %testo.caddon )) { unset %testo.caddon | goto finalectura }
  echo -s %testo.caddon
  unset %testo.caddon
  goto lecturaddon
  :finalectura
  sockclose addonnew
}
